{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "labels":{
      "app": "mypod"
    },
    "name": "ss",
    "namespace": "default"
  },
  "spec": {
    "containers": [
      {
        "image": "nginx:1.18-alpine",
        "imagePullPolicy": "IfNotPresent",
        "name": "nginx",
        "ports": [
          {
            "containerPort": 80
          }
        ]
      }
    ]
  }
}
